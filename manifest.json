{
  // "$schema": "https://json.schemastore.org/chrome-manifest",
  "manifest_version": 3,
  "name": "NPM Package Size Extension",
  "version": "0.1",
  "description": "This Extension adds Bundlephobia Minzip size badge to the corresponding NPM package page",

  "content_scripts": [
    {
      "js": ["main.js"],
      "matches": ["https://www.npmjs.com/package/*"]
    }
  ]
}
